{
  "name": "patrickrobrecht/wp-dev",
  "description": "WordPress Plugins Dashboard",
  "license": "GPL-3.0",
  "require": {
    "php": "^8.5",
    "ext-json": "*",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "portavice/laravel-pint-config": "^3.0"
  },
  "scripts": {
    "cs": [
      "pint --config vendor/portavice/laravel-pint-config/pint.json --test"
    ],
    "csfix": [
      "pint --config vendor/portavice/laravel-pint-config/pint.json"
    ],
    "prod": [
      "@composer install --optimize-autoloader --no-dev"
    ]
  },
  "autoload": {
    "psr-4": {
      "WordPressPluginDashboard\\": "src/"
    }
  },
  "config": {
    "audit": {
      "abandoned": "fail"
    }
  }
}
